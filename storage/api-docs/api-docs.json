        "/searchhotels": {
            "post": {
                "tags": [
                    "Search"
                ],
                "summary": "Get Data By Search Hotels Form (sorting by stars (desc))",
                "operationId": "",
                "parameters": [
                    {
                        "name": "city_name",
                        "in": "query",
                        "description": "hurghada or الغردقه or empty (all cities)",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "check_in",
                        "in": "query",
                        "description": "like:(Y-m-d) 2019-11-23",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "check_out",
                        "in": "query",
                        "description": "like:(Y-m-d) 2019-11-29",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "adult",
                        "in": "query",
                        "description": "to show rooms just (single or double or triple)",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "nationality",
                        "in": "query",
                        "description": "User Nationality to Used it in PriceList. like (US-EG...) max:2 char",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "locale",
                        "in": "query",
                        "description": "data language (En-Ar)",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "user_type",
                        "in": "query",
                        "description": "(guest or trader) used it priceList ",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "400": {
                        "description": "Invalid request"
                    },
                    "404": {
                        "description": "not found"
                    }
                }
            }
